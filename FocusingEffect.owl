    <!-- http://www.leonardozilli.it/CognitiveBiasOntology/FocusingEffect.owl#DecisionOutcome -->

    <owl:Class rdf:about="http://www.leonardozilli.it/CognitiveBiasOntology/FocusingEffect.owl#DecisionOutcome">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.leonardozilli.it/CognitiveBiasOntology/FocusingEffect.owl#Option"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/componency.owl#hasComponent"/>
                        <owl:someValuesFrom rdf:resource="http://www.leonardozilli.it/CognitiveBiasOntology/FocusingEffect.owl#SalientDetail"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.leonardozilli.it/CognitiveBiasOntology/FocusingEffect.owl#Option"/>
        <rdfs:comment>The result of an act of decision.</rdfs:comment>
    </owl:Class>
    


