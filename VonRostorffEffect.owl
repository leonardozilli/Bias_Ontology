    <!-- https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#observe_enhanced_info -->

    <owl:NamedIndividual rdf:about="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#observe_enhanced_info">
        <rdf:type rdf:resource="http://modellingdh.github.io/ont/odp/term/Observation"/>
        <term:isReflectionOn rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying_technique1"/>
        <rdfs:label>An individual is more likely to remember an item that stands out from a group of similar items.</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#student_1 -->

    <owl:NamedIndividual rdf:about="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#student_1">
        <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <term:isEngagedIn rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying_technique1"/>
        <rdfs:label>Maria</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying -->

    <owl:NamedIndividual rdf:about="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying">
        <rdf:type rdf:resource="http://etna.istc.cnr.it/framester2/data/framestercore/Studying"/>
        <term:includes rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#observe_enhanced_info"/>
        <rdfs:label>devote time and attention to gaining knowledge of (an academic subject), especially by means of books.</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying_technique1 -->

    <owl:NamedIndividual rdf:about="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying_technique1">
        <rdf:type rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/StudyTechnique"/>
        <term:inActivity rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#studying"/>
        <leverages rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#von_rostorff_effect"/>
        <owl1:parameter.owl>high</owl1:parameter.owl>
        <rdfs:label>highlighting key points during studies.</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#von_rostorff_effect -->

    <owl:NamedIndividual rdf:about="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#von_rostorff_effect">
        <rdf:type rdf:resource="http://etna.istc.cnr.it/framester2/data/framestercore/SubjectiveInfluence"/>
        <term:producedObservation rdf:resource="https://github.com/leonardozilli/Bias_Ontology/VonRostorffEffect/#observe_enhanced_info"/>
        <rdfs:comment>The Von Restorff effect, also known as the &quot;isolation effect&quot;, is a psychological phenomenon where an individual is more likely to remember an item that stands out from a group of similar items. This effect was first identified by Hedwig von Restorff in 1933. The principle behind this effect is that the item that differs from the rest is more memorable due to its increased salience.</rdfs:comment>
        <rdfs:label>Von Rostorff effect</rdfs:label>
    </owl:NamedIndividual>
    


