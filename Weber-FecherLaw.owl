###  http://www.leonardozilli.it/CognitiveBiasOntology/Weber-FecherLaw.owl#typeOfDifference
:typeOfDifference rdf:type owl:DatatypeProperty ;
                  rdfs:domain :PerceivedDifference ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "notSignificant" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "significant" ;
                                                      rdf:rest rdf:nil
                                                    ]
                                         ]
                             ] .


