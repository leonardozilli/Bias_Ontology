    <owl:Class rdf:about="http://www.leonardozilli.it/CognitiveBiasOntology/HumorEffect#HumorousContent">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.leonardozilli.it/CognitiveBiasOntology/HumorEffect#MentalContent"/>
                    <owl:Restriction>
